---
import SiteLayout from "@layouts/SiteLayout.astro";
import GridLayout from "@layouts/GridLayout.astro";
import Grid from "@components/Grid.astro";
import { fall2023 } from "@editions/fall2023";
---

<SiteLayout title="Fall 2023 | -ette review">
  <GridLayout
    issueTitle={"Issue I: Fall 2023"}
    issue={"archive/fall2023"}
    authors={fall2023}
  >
    <Grid writers={fall2023} slug="archive/fall2023" />
  </GridLayout>
</SiteLayout>

<style>
  :root {
    --clr-bg-primary: var(--clr-bg-fall2023);
  }
</style>
